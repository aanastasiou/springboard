# Basic stack based programming functions
#
: drop <[-];
: swap <[->+>+<<]<[->+<]>>>[-<<<+>>>]<;
: dup <[->+<]>[-<+>>+<]>[-<+>];
: over dup dup << [-] > [-] <<<[->>+>+<<<]>>>[-<<<+>>>] ;
: rot <<<[->>>+<<<]>[-<+>]>[-<+>]>[-<+>];
: write [-]<[->>[>]+[<]<]<[->>>[>]>+<<[<]<]>>[>]<[-<]<< ;
: read <[->>[>]+[<]<]>>[>]>[-<<[<]<+>>[>]>]<<[-<] ;
: if dup not swap<[[-] ;
: then ] ;
