# Basic stack based programming functions
#
import "bool.sb"

: drop [-]<[-];
: swap <[->+>+<<]<[->+<]>>>[-<<<+>>>]<[-];
: dup <[->+<]>[-<+>>+<]>[-<+>];
: over dup dup << [-] > [-] <<<[->>+>+<<<]>>>[-<<<+>>>] ;
: rot <<<[->>>+<<<]>[-<+>]>[-<+>]>[-<+>];
: if  isnonzero dup not swap < [ [-]<[0]  ;
: else 0 0] < [[-] ;
: then 0] drop ;
: write [-]<[->>[>]+[<]<]<[->>>[>]>+<<[<]<]>>[>]<[-<]<< ;
: read <[->>[>]+[<]<]>>[>]>[-<<[<]<+>>[>]>]<<[-<] ;
