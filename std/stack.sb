# Basic stack based programming functions
#

: drop <[-];
: swap <[->+>+<<]<[->+<]>>>[-<<<+>>>]<[-];
: dup <[->+<]>[-<+>>+<]>[-<+>];
: over dup dup << [-] > [-] <<<[->>+>+<<<]>>>[-<<<+>>>] ;
: rot <<<[->>>+<<<]>[-<+>]>[-<+>]>[-<+>];
: write [-]<[->>[>]+[<]<]<[->>>[>]>+<<[<]<]>>[>]<[-<]<< ;
: read <[->>[>]+[<]<]>>[>]>[-<<[<]<+>>[>]>]<<[-<] ;
: pave <[->>[>]+[<]<]>>[>]>[-<<[<]<+>>[>]>]<<[<]<;
: fetchright [-]>[>]>[-<<[<]<+>[>]>];
